  "$defs": {
      "schemas": {
          "groupPartnershipSubscriptionType": {
              "description": "groupPartnershipSubscription is mandatory when GroupSubscription is true or PartnershipSubscription is true",
              "type": "object",
              "properties": {
              "representativeControl": {
                  "type": "boolean"
              },
              "allMembersControl": {
                  "type": "boolean"
              },
              "groupPartnershipDetails": {
                  "type": "array",
                  "minItems": 1,
                  "items": {
                  "$ref": "#/$defs/schemas/groupPartnershipDetailsType"
                  }
              }
              },
              "required": [
              "groupPartnershipDetails"
              ],
              "additionalProperties": false
          },
          "customerIdentificationType": {
              "description": "Customer Identification Number. Allows digits, upper and lowercase alphabetic characters min 1, max 15 characters",
              "type": "string",
              "pattern": "^[a-zA-Z0-9]{1,15}$"
          },
          "monetaryPos": {
              "type": "number",
              "minimum": 0,
              "maximum": 99999999999.99,
              "multipleOf": 0.01,
              "description": "Defines a monetary value (to 2 decimal places), between 0 and 99999999999.99"
          },
          "groupPartnershipDetailsType": {
              "type": "object",
              "properties": {
              "relationship": {
                  "type": "string",
                  "pattern": "^[A-Za-z0-9 ,.()/&'-]{1,14}$"
              },
              "customerIdentification1": {
                  "$ref": "#/$defs/schemas/customerIdentificationType"
              },
              "customerIdentification2": {
                  "$ref": "#/$defs/schemas/customerIdentificationType"
              },
              "organisationDetails": {
                  "$ref": "#/$defs/schemas/organisationDetailsType"
              },
              "individualDetails": {
                  "$ref": "#/$defs/schemas/individualDetailsType"
              },
              "addressDetails": {
                  "$ref": "#/$defs/schemas/addressDetailsType"
              },
              "contactDetails": {
                  "$ref": "#/$defs/schemas/contactDetailsType"
              }
              },
              "additionalProperties": false,
              "required": [
              "relationship",
              "customerIdentification1",
              "addressDetails",
              "contactDetails"
              ]
          },
          "organisationNameType": {
              "type": "string",
              "pattern": "^[a-zA-Z0-9À-ÿ !#$%&'‘’\"“”«»()*+,./:;=?@\\[\\]£€¥\\\\—–‐-]{1,160}$",
              "description": "Organisation Name"
          },
          "dateType": {
              "type": "string",
              "pattern": "^(((19|20)([2468][048]|[13579][26]|0[48])|2000)[-]02[-]29|((19|20)[0-9]{2}[-](0[469]|11)[-](0[1-9]|1[0-9]|2[0-9]|30)|(19|20)[0-9]{2}[-](0[13578]|1[02])[-](0[1-9]|[12][0-9]|3[01])|(19|20)[0-9]{2}[-]02[-](0[1-9]|1[0-9]|2[0-8])))$"
          },
          "addressLineType": {
              "description": "Address Line , — for — (EM DASH), – for – (EN DASH), ‐ for -(HYPHEN), - for -(HYPHEN_MINUS)",
              "type": "string",
              "pattern": "^[A-Za-z0-9 &!'‘’\"“”(),./—–‐-]{1,35}$"
          },
          "nameType": {
              "type": "string",
              "pattern": "^[A-Za-z0-9 ,.()/&'-]{1,35}$"
          },
          "individualDetailsType": {
              "type": "object",
              "properties": {
              "title": {
                  "type": "string",
                  "enum": [
                  "Mr",
                  "Mrs",
                  "Miss",
                  "Ms",
                  "Dr",
                  "Sir",
                  "Professor",
                  "Lord"
                  ]
              },
              "firstName": {
                  "$ref": "#/$defs/schemas/nameType"
              },
              "middleName": {
                  "$ref": "#/$defs/schemas/nameType"
              },
              "lastName": {
                  "$ref": "#/$defs/schemas/nameType"
              }
              },
              "required": [
              "firstName",
              "lastName"
              ],
              "additionalProperties": false
          },
          "organisationDetailsType": {
              "type": "object",
              "properties": {
              "organisationType": {
                  "type": "string",
                  "pattern": "^[A-Za-z0-9 ,.()/&'-]{1,40}$"
              },
              "organisationName": {
                  "$ref": "#/$defs/schemas/organisationNameType"
              }
              },
              "additionalProperties": false,
              "required": [
              "organisationName"
              ]
          },
          "principalPlaceOfBusinessDetailsType": {
              "type": "object",
              "properties": {
              "addressDetails": {
                  "$ref": "#/$defs/schemas/addressDetailsType"
              },
              "contactDetails": {
                  "$ref": "#/$defs/schemas/contactDetailsType"
              }
              },
              "additionalProperties": false,
              "required": [
              "addressDetails"
              ]
          },
          "primaryContactDetailsType": {
              "type": "object",
              "properties": {
              "name": {
                  "$ref": "#/$defs/schemas/organisationNameType"
              },
              "positionInCompany": {
                  "$ref": "#/$defs/schemas/organisationNameType"
              },
              "contactDetails": {
                  "$ref": "#/$defs/schemas/contactDetailsType"
              }
              },
              "required": [
              "name",
              "positionInCompany",
              "contactDetails"
              ],
              "additionalProperties": false
          },
          "declarationType": {
              "type": "object",
              "properties": {
              "declarationBox1": {
                  "type": "boolean"
              }
              },
              "required": [
              "declarationBox1"
              ],
              "additionalProperties": false
          },
          "addressDetailsType": {
              "oneOf": [
              {
                  "type": "object",
                  "properties": {
                  "addressLine1": {
                      "$ref": "#/$defs/schemas/addressLineType"
                  },
                  "addressLine2": {
                      "$ref": "#/$defs/schemas/addressLineType"
                  },
                  "addressLine3": {
                      "$ref": "#/$defs/schemas/addressLineType"
                  },
                  "addressLine4": {
                      "$ref": "#/$defs/schemas/addressLineType"
                  },
                  "countryCode": {
                      "type": "string",
                      "enum": [
                      "GB"
                      ]
                  },
                  "postalCode": {
                      "type": "string",
                      "pattern": "^[A-Z]{1,2}[0-9][0-9A-Z]?\\s?[0-9][A-Z]{2}$|BFPO\\s?[0-9]{1,3}$"
                  }
                  },
                  "required": [
                  "addressLine1",
                  "addressLine2",
                  "countryCode",
                  "postalCode"
                  ],
                  "additionalProperties": false
              },
              {
                  "type": "object",
                  "properties": {
                  "addressLine1": {
                      "$ref": "#/$defs/schemas/addressLineType"
                  },
                  "addressLine2": {
                      "$ref": "#/$defs/schemas/addressLineType"
                  },
                  "addressLine3": {
                      "$ref": "#/$defs/schemas/addressLineType"
                  },
                  "addressLine4": {
                      "$ref": "#/$defs/schemas/addressLineType"
                  },
                  "countryCode": {
                      "$ref": "#/$defs/schemas/countryCodes"
                  },
                  "postalCode": {
                      "type": "string",
                      "minLength": 1,
                      "maxLength": 10
                  }
                  },
                  "required": [
                  "addressLine1",
                  "addressLine2",
                  "countryCode"
                  ],
                  "additionalProperties": false
              }
              ]
          },
          "contactDetailsType": {
              "type": "object",
              "properties": {
              "telephone": {
                  "$ref": "#/$defs/schemas/phoneNumberType"
              },
              "mobileNumber": {
                  "$ref": "#/$defs/schemas/phoneNumberType"
              },
              "email": {
                  "type": "string",
                  "minLength": 3,
                  "maxLength": 132
              }
              },
              "additionalProperties": false,
              "required": [
              "telephone",
              "email"
              ]
          },
          "phoneNumberType": {
              "type": "string",
              "minLength": 1,
              "maxLength": 24,
              "pattern": "^[0-9 )/(*#+-]+$"
          },
